{% set pagetitle = 'Authentication problem'|trans %}
{% extends "base.twig" %}
{% block content %}

{% if errorcode -%}
<h2>{% trans 'Error' %}</h2>
<h3>{% trans errortitle %}</h3>
<p>{% trans errordescr %}</p>
{% endif -%}

<h2>{% trans 'Authentication problem' %}</h2>

<p>{% trans 'You haven't been found in the list of authorized users.' %}</p>

<a href="{{ loginurl }}">
{% trans 'Login' %}
</a>

{% endblock %}
